<UserControl x:Class="HeroesParserData.Views.Match.LastMatchControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HeroesParserData.Views.Match"
             xmlns:ViewModels="clr-namespace:HeroesParserData.ViewModels.Match"
             mc:Ignorable="d" 
             HorizontalAlignment="Left"
             VerticalAlignment="Top"
             d:DataContext="{d:DesignInstance ViewModels:LastGameViewModel}">
    <UserControl.DataContext>
        <ViewModels:LastGameViewModel />
    </UserControl.DataContext>
    <DockPanel>
        <Grid>
            <Button Content="Refresh" Command="{Binding Refresh}" Width="100" Height="28" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,9,-105,0" />
            <local:MatchSummaryControl DockPanel.Dock="Bottom"></local:MatchSummaryControl>
        </Grid>
    </DockPanel>
</UserControl>
