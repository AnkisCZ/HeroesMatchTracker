<UserControl x:Class="HeroesStatTracker.Views.Matches.MatchesControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HeroesStatTracker.Views.Matches"
             xmlns:dragablz="clr-namespace:Dragablz;assembly=Dragablz"
             mc:Ignorable="d" 
             d:DesignHeight="1080" d:DesignWidth="1920">
    <dragablz:TabablzControl>
        <TabItem Header="All Matches">
            <local:AllMatchesControl />
        </TabItem>
        <TabItem Header="Quick Match">
            <local:QuickMatchControl />
        </TabItem>
        <TabItem Header="Unranked Draft">
            <local:UnrankedDraftControl />
        </TabItem>
        <TabItem Header="Hero League">
            <local:HeroLeagueControl />
        </TabItem>
        <TabItem Header="Team League">
            <local:TeamLeagueControl />
        </TabItem>
        <TabItem Header="Brawl">
            <local:BrawlControl />
        </TabItem>
        <TabItem Header="Custom Game">
            <local:CustomGameControl />
        </TabItem>
    </dragablz:TabablzControl>
</UserControl>
